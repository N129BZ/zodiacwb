{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Run npm start",
            "type": "node-terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}\\main.js",
            "console": "integratedTerminal"
          }
    ]
}
